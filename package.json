{
  "name": "turnjs-vue",
  "version": "0.0.2",
  "description": "fork from turn.js but support vue2",
  "main": "turn.js",
  "scripts": {
    "build": "rollup -c scripts/rollup.config.prod.mjs --bundleConfigAsCjs",
    "build:watch": "rollup -c scripts/rollup.config.prod.mjs --watch --bundleConfigAsCjs",
    "dev": "rollup -c scripts/rollup.config.dev.mjs -w --bundleConfigAsCjs",
    "release": "sh scripts/release.sh",
    "changelog": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehxie/turn.js.git"
  },
  "author": "ehxie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ehxie/turn.js/issues"
  },
  "homepage": "https://github.com/ehxie/turn.js#readme",
  "dependencies": {
    "@babel/core": "^7.22.9",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "jquery": "2",
    "rollup": "^3.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "select-version-cli": "^0.0.2",
    "standard-version": "^9.5.0",
    "vue": "2.7.14",
    "vue-template-compiler": "^2.7.14"
  }
}
